{
  "presets": [ ["env", { "modules": "commonjs" }], "react", "flow" ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "transform-class-properties",
    "transform-object-rest-spread",
    "add-module-exports",
    "transform-decorators-legacy"
  ]
}
